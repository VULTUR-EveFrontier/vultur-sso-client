  "packageManager": "pnpm@10.12.1",
  "peerDependencies": {
    "react": "^18.0.0",
    "next": "^14.0.0",
    "@tanstack/react-query": "^5.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/react": "^18.2.0",
    "@types/node": "^20.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
